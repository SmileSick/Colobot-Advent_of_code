extern void object::AoD_2015_01()
{
	ipf(10000);
	int part1 = 0;
	int part2;
	bool check = true;
	file handle();
	handle.open("input.txt", "r");
	string text = handle.readln();
	handle.close();
	int count = strlen(text);
	string temptext;
	for(int i = 0; i < count; i++)
	{
		temptext = strmid(text, i, 1);
		if(temptext == "(") part1++;
		if(temptext == ")") part1--;
		if(part1 == -1 && check)
		{
			part2 = i + 1;
			check = false;
		}
	}
	message("Part1: " + part1 + ", Part2: " + part2);
}
