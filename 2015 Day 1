extern void object::AoD_2015_1()
{
	ipf(10000);
	int p1 = 0;
	int p2;
	bool check = true;
	file handle = new file();
	handle.open("input.txt", "r");
	string text = handle.readln();
	handle.close();
	handle = null;
	int count = strlen(text);
	string temp;
	for(int i = 0; i < count; i++)
	{
		temp = strmid(text, i, 1);
		if(temp == "(") p1++;
		else p1--;
		if(p1 == -1 && check)
		{
			p2 = i + 1;
			check = false;
		}
	}
	message("Part: " + p1 + ", Part: " + p2);
}
